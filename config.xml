<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.geolocation" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>ShuOu</name>
	<description>Phonegap Cordova geolocation</description>
	<author email="mmmarcjr@gmail.com" href="http://cordova.io">Marc Morin</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
	<plugin name="cordova-plugin-whitelist" source="npm" version="~1" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="Fullscreen" value="true" />
	<access origin="*" />
	<allow-intent href="http://code.jquery.com/*" />
	<allow-intent href="http://maps.google.com/*" />
	<allow-navigation href="http://maps.google.com/*" />
	<platform name="android">
			<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>
